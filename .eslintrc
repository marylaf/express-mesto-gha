{ "plugins": [
  "editorconfig"
],
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "ignore",
      "imports": "never",
      "exports": "never",
      "functions": "never"
  }],
    "quotes": ["error", "double"],
    "no-underscore-dangle": ["error", {"allow": ["_id"]}]
  }
}